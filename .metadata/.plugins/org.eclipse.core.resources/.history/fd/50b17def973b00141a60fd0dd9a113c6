
public class ghost {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String toTest = "this is a test";
		te_replacer(toTest);
	}
	
	static void te_replacer(String string) {
		
		for (int i = 0; i < string.length(); i++) {
			
			System.out.println(string.charAt(i));
			
		}
		
		
		
	}
	
	
}
